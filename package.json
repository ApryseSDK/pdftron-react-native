{
  "name": "react-native-pdftron",
  "title": "React Native Pdftron",
  "version": "3.0.2-beta.19",
  "description": "React Native Pdftron",
  "main": "./lib/index.js",
  "typings": "index.ts",
  "scripts": {
    "copy-to-repo": "cd scripts && python3 fileCopy.py toRepo && cd ..",
    "copy-to-node-modules": "cd scripts && python3 fileCopy.py toModule && cd ..",
    "start": "rm -rf docs && tsc && ./node_modules/.bin/jsdoc -c conf.json",
    "upload-docs": "cd scripts && python3 -m pip install pwinput && python3 buildDocs.py $BRANCH && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PDFTron/pdftron-react-native.git",
    "baseUrl": "https://github.com/PDFTron/pdftron-react-native"
  },
  "keywords": [
    "react-native",
    "PDFTron",
    "pdf",
    "office"
  ],
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "prop-types": "*"
  },
  "devDependencies": {
    "react": "*",
    "@types/react": "*",
    "@types/react-native": "*",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "better-docs": "^2.3.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "ink-docstrap": "git+https://github.com/akwanpdf/docstrap.git#pdftron-react-native",
    "jsdoc": "^3.6.7",
    "typescript": "*"
  }
}
